\def\@makechapterhead#1{{\parindent 0pt
 \begin{center}
  %  \ifnum \c@secnumdepth >\m@ne \bf \@chapapp{} \thechapter \\ % TCM use this line to bold Chapter and Chapter Name
  \ifnum \c@secnumdepth >\m@ne  \@chapapp{} \thechapter \\ % TCM use this line to NOT bold Chapter and Chapter Name
   \vskip 2pt \fi    % TCM Increase space between chapter and chapter name
   %     \bf          %% Julie Use to boldface chapter name only
   #1
  \end{center}
  \nobreak } }

 %-------------------------------------------------------------
 % BEGIN TOC, LOF, LOT Chapter Headings Customization
 %-------------------------------------------------------------
 \def\@makeschapterhead#1{{\parindent 0pt
  \begin{center}
   %     \bf          %% Julie Use to boldface TOC, LOF, LOT chapter names
   #1
  \end{center}
  \nobreak } }
 %-------------------------------------------------------------
 % END TOC, LOF, LOT Chapter Headings Customization
 %-------------------------------------------------------------


 \def\chapter{\clearpage \thispagestyle{plain} \global\@topnum\z@
  \@afterindentfalse \secdef\@chapter\@schapter}

 \def\@chapter[#1]#2{\ifnum \c@secnumdepth >\m@ne
  \advance \numfigure by \c@figure
  \advance \numtable by \c@table
  \refstepcounter{chapter}
  \typeout{\@chapapp\space\thechapter.}
  \addcontentsline{toc}{chapter}{\protect
   % \numberline{\hfill \thechapter. \hbox{\hskip 0.8em}\ignorespaces}#1}\else % TCM This code adds a ''0.8em'' in the PDF navigation tree for chapters but no error in the document.  Changed to line below. Not exactly equivalent code, but removes the ''0.8em'' in the PDF tree with no apparent side effects.
   \numberline{\hfill \thechapter. \hbox{\hspace{0.1in}}\ignorespaces}#1}\else
  \addcontentsline{toc}{chapter}{#1}\fi
  % \vbox{\vskip 0.745in} %% Julie space betw. top of page and 'CHAPTER' if use plain
  % \vbox{\vskip 0.66in} %% Julie space betw. top of page and 'CHAPTER' if use Bold
  \vbox{\vskip 0.50in} %% saeed space betw. top of page and 'CHAPTER'
  \chaptermark{#1}

  %************************NEW CHAPTER DEFINITION*****************
  \if@twocolumn
  \@topnewpage[\@makechapterhead{#2}]
  \else \@makechapterhead{#2}
  \@afterheading\fi
  % \vskip -0.27in  %% Julie add space betw. Plain chapter name and 1st sentence
  \vskip -0.24in  %% Julie add space betw. Bold chapter name and 1st sentence
  \chaptertest=0
  \indent
  \everypar={\advance \chaptertest by 1}
 }

 \def\@schapter#1{\if@twocolumn \@topnewpage[\@makeschapterhead{#1}]
  \else \@makeschapterhead{#1}
  \@afterheading\fi
  \indent
 }

